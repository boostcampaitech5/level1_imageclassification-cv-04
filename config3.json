{
    "model":{
        "name": "mask_classification3",
        "args":{
            "loss": "Custom",
            "class_num": 18
        }
    },
    "dataset":{
        "split": false,
        "split_ratio": 0.8,

        "name": "mask_dataset3",
        "args": {
            "base_dir":"../input2/data"
        },
        "annotation_dir": "/annotation"
    },
    "dataloader":{
        "batch_size": 32,
        "shuffle": true
    },
    "epochs": 20,
    "optimizer":{
        "name": "Adam",
        "args":{
            "lr": 0.001
        }
    },
    "backbone":{
        "name": "Resnet50"
    },
    "save_name":"resnet_cutting_full_train"
}