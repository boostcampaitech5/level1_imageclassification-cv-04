{
	"resize": {
		"img_width": 224,
		"img_height": 224
	},
	"totensor": {},
	"normalize": {
		"normalize_statistic": "imagenet"
	},
	"centercrop": {
		"img_width": 384,
		"img_height": 384
	},
	"colorjitter": {
		"brightness": 0,
		"contrast": [1.8, 2.0],
		"saturation": 0,
		"hue": 0
	},
	"randomhorizontalflip": {
		"flip_prob": 0.5
	},
	"randomrotation": {
		"degrees": 20
	},
	"gaussianblur": {
		"kernel_size": 3,
		"sigma_min": 0.1,
		"sigma_max": 2.0
	},
	"randomaffine": {
		"degrees":30,
		"shear":[-10,10,-10,10],
		"translate":[0.1, 0.1]
	}
}