{
    "model":{
        "name": "mask_classification",
        "args": {
            "loss": "Custom",
            "class_num": 6
        }
    },
    "dataset":{
        "split": false,
        "split_ratio": 0.8,

        "name": "mask_dataset",
        "args": {
            "base_dir":"../input2/data"
        },
        "annotation_dir": "/annotation"
    },
    "dataloader":{
        "batch_size": 32,
        "shuffle": true
    },
    "epochs": 20,
    "optimizer":{
        "name": "Adam",
        "args":{
            "lr": 0.005,
            "weight_decay":0.01
        }
    },
    "backbone":{
        "name": "Resnet50"
    },
    "save_name":"test4"
}